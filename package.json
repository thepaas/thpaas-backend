{
  "name": "thepaas-backend",
  "version": "0.0.1",
  "description": "Backend",
  "author": "Eugene Voronov (https://github.com/eugenvoronov)",
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write 'src/**/*.ts' 'test/**/*.ts'",
    "lint": "eslint 'src/**/*.ts' 'test/**/*.ts' --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/src/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "migration:create": "yarn build && typeorm-ts-node-commonjs migration:create",
    "migration:generate": "yarn build && typeorm-ts-node-commonjs migration:generate -p -d typeorm.config.ts",
    "migration:revert": "yarn build && typeorm-ts-node-commonjs migration:revert -d typeorm.config.ts",
    "migration:run": "yarn build && typeorm-ts-node-commonjs migration:run -d typeorm.config.ts",
    "migration:show": "yarn build && typeorm-ts-node-commonjs migration:show -d typeorm.config.ts"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.2.7",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.10",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/mapped-types": "^2.0.5",
    "@nestjs/passport": "^10.0.0",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/platform-socket.io": "^11.0.3",
    "@nestjs/schedule": "^4.0.1",
    "@nestjs/serve-static": "^4.0.1",
    "@nestjs/swagger": "^7.1.13",
    "@nestjs/terminus": "^10.2.1",
    "@nestjs/typeorm": "^10.0.1",
    "@nestjs/websockets": "^11.0.3",
    "@sendgrid/mail": "^8.1.3",
    "@types/passport-jwt": "^4.0.1",
    "async-mutex": "^0.5.0",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "decimal.js": "^10.4.3",
    "ethers": "^6.13.4",
    "helmet": "^7.1.0",
    "joi": "^17.13.3",
    "json-stable-stringify": "^1.1.1",
    "nestjs-minio-client": "^2.2.0",
    "oidc-provider": "^9.1.1",
    "openpgp": "^6.0.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pg": "8.12.0",
    "qrcode": "^1.5.4",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.8.1",
    "starknet": "^6.24.1",
    "typeorm": "^0.3.17",
    "typeorm-naming-strategies": "^4.1.0",
    "uuid": "^10.0.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.5.0",
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.1",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.12",
    "@types/json-stable-stringify": "^1.0.36",
    "@types/node": "22.5.4",
    "@types/oidc-provider": "^9.1.0",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "@types/xml2js": "0.4.13",
    "@types/zxcvbn": "4.4.4",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vercel/node": "^3.0.26",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "29.7.0",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.20",
    "supertest": "^7.0.0",
    "ts-jest": "29.2.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write",
      "eslint --fix"
    ]
  }
}
